!-------------------------------------------------------------------------------
!
!
!      X F T
!
!
Xft.antialias: 1
Xft.dpi:       96
Xft.hinting:   1
Xft.hintstyle: hintfull
Xft.rgba:      rgb


!-------------------------------------------------------------------------------
!
!
!      E M A C S
!
! Most settings deprecated since gtk2 support, and
! Color-Theme package takes care of coloring now.
!Emacs*font:       -xos4-terminus-medium-r-normal-*-12-120-72-72-c-60-iso8859-2
!Emacs*Background: #262626
!Emacs*Foreground: #bdb76b
!Emacs*Background: gray15
!Emacs*Foreground: darkkhaki


!-------------------------------------------------------------------------------
!
!	U R X V T
!
! General
!urxvt*geometry:         80x26
urxvt.termName:  rxvt-256color
urxvt.loginShell:         true
urxvt.scrollBar:         false
urxvt.internalBorder:        0
urxvt.borderWidth:           0
urxvt.secondaryScroll:    true
urxvt.saveLines:         65535
urxvt.cursorBlink:        true
!
! Extensions
urxvt*perl-lib:         /usr/lib/urxvt/perl/
urxvt*perl-ext-common:  default,matcher,searchable-scrollback
urxvt*urlLauncher:      /usr/local/bin/firefox
urxvt*matcher.button:   1
! So we catch even nasty URLs
urxvt.cutchars:         `()'*<>[]{|}"'
URxvt*cutchars:         `"()'*<>[]{|}"
!
! Appearance
urxvt.font:     -xos4-terminus-medium-*-*-*-12-*-*-*-*-*-*-*
urxvt.boldFont: -xos4-terminus-medium-*-*-*-12-*-*-*-*-*-*-*
!urxvt.font:        xft:Bitstream Vera Sans Mono:pixelsize=10:autohint=true
!urxvt.boldFont:    xft:Bitstream Vera Sans Mono:bold:pixelsize=10:autohint=true

!
urxvt.transparent: false
!urxvt.shading:    30
urxvt.cursorColor: #FFFFFF
urxvt*faceName:      terminus:pixelsize=8

! 
! Zenburn
urxvt*background:  #3f3f3f
urxvt*foreground:  #dcdccc
! black + red
