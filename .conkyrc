background no
out_to_x no
out_to_console yes
update_interval 1
total_run_times 0
uppercase no
no_buffers yes
#use_spacer none

TEXT
${cpu cpu}% || ${acpitemp}c || ${battery} || $memperc% ($mem) || d:${downspeed wlan0}K u:${upspeed wlan0}K || ${time %a %b %d %H:%M}
